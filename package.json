{
  "name": "kn-ui",
  "version": "0.5.0",
  "private": false,
  "main": "packages/index.js",
  "module": "dist/lib.common.js",
  "style": "dist/lib.css",
  "files": [
    "dist",
    "packages"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:packages": "vue-cli-service build --target lib --name lib packages/index.js",
    "lint": "vue-cli-service lint",
    "create": "node ./scripts/create/",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "open tests/coverage/index.html"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "jsdoc": "^3.5.5",
    "markdown-it-container": "^2.0.0",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/polyfill": "^7.0.0",
    "@vue/cli-plugin-babel": "^3.1.0",
    "@vue/cli-plugin-eslint": "^3.1.0",
    "@vue/cli-service": "^3.1.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-import": "^1.11.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.8.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-vue": "^5.0.0-0",
    "jest": "^23.6.0",
    "jest-serializer-vue": "^2.0.2",
    "lint-staged": "^7.2.2",
    "minimist": "^1.2.0",
    "node-sass": "^4.10.0",
    "nyc": "^13.1.0",
    "postcss": "^7.0.5",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "vue-jest": "^3.0.1",
    "vue-markdown-loader": "^2.4.1",
    "vue-template-compiler": "^2.5.17"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "nyc": {
    "include": [
      "packages/**/*.js"
    ],
    "instrument": false,
    "sourceMap": false
  }
}
